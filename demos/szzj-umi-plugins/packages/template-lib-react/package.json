{
  "name": "@szzj/template-lib-react",
  "version": "0.2.0",
  "maintainers": [
    {
      "name": "shengchao.qiushch",
      "email": "shengchao.qiushch@dtzhejiang.com"
    },
    {
      "name": "lixin",
      "email": "xin.lix@dtzhejiang.com"
    },
    {
      "name": "zhangw",
      "email": "wei.zhang@dtzhejiang.com"
    },
    {
      "name": "liuliu.yull",
      "email": "liuliu.yull@dtzhejiang.com"
    },
    {
      "name": "gaoll",
      "email": "lili.gao@dtzhejiang.com"
    },
    {
      "name": "wangyanan",
      "email": "yanan.wangyn@dtzhejing.com"
    }
  ],
  "sideEffects": [
    "*.less",
    "*.css",
    "*.png"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/types.d.ts",
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "dev": "dumi dev",
    "docs:build": "cross-env DOCS_ENV=prod dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "dumi lint --stylelint-only \"{src,test}/**/*.{css,less}\"",
    "lint:es": "dumi lint --eslint-only \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier -c **/*",
    "prettier:write": "prettier -w **/*",
    "start": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@szzj/spec": "~1.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "dumi": "^2.1.23",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "styled-components": "^5.0.0",
    "typescript": "~4.7.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
